set_progress(0.0);
ui_print("");
ui_print("Installing Kernel...");

package_extract_dir("boot", "/tmp/boot");

set_progress(0.1);

ui_print("Installing Kernel");

run_program("/sbin/dd", "if=/tmp/boot/boot.img", "of=/dev/block/mmcblk0p42"));

set_progress(0.9);

ui_print("");
ui_print("");
set_progress(1.0);
ui_print("Finished!");
